        "karma-cljs-test": "^0.1.0",
        "npm-run-all": "^4.1.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-grid-layout": "^0.16.6",
        "react-icons": "5.0.1",
        "shadow-cljs": "^2.16.12",
        "showdown": "^1.9.1"
    },
